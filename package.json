{
  "name": "@creadigme/au-i18n-audit",
  "version": "999.0.0",
  "description": "Aurelia I18N Audit brings more control over your translations. With the integrate CLI you can check if all your translations keys is used, more importantly, you will be able to find the ones that are not defined.",
  "author": "CREADIGME",
  "license": "MIT",
  "main": "dist/au-i18n-audit.bundle",
  "bin": {
    "au-i18n-audit": "./dist/au-i18n-audit-cli.js",
    "aurelia-i18n-audit": "./dist/au-i18n-audit-cli.js"
  },
  "typings": "dist/bundle.d.ts",
  "engines": {
    "node": ">=16.14"
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/creadigme/au-i18n-audit",
  "pre-commit": [
    "lint"
  ],
  "publishConfig": {
    "@creadigme:registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "aurelia",
    "i18n",
    "i18next",
    "lint",
    "audit",
    "validator"
  ],
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "bundle-declarations-webpack-plugin": "^2.2.2",
    "codecov": "^3.8.3",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "json": "^11.0.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.7",
    "ts-mocha": "^9.0.2",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "command-line-args": "^5.2.1",
    "exceljs": "^4.3.0",
    "fast-glob": "^3.2.11",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2"
  },
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin ./dist && node ./node_modules/rimraf/bin ./coverage && node ./node_modules/rimraf/bin ./.nyc_output && node ./node_modules/rimraf/bin ./node_modules",
    "lint": "node ./node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "node ./node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx} --fix",
    "test": "node ./node_modules/ts-mocha/bin/ts-mocha --timeout 15000 ./src/**/*.spec.ts",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=json --reporter=text-summary npm test",
    "analyze": "node  ./node_modules/rimraf/bin dist && node ./node_modules/webpack/bin/webpack --env production --analyze",
    "build": "node ./node_modules/rimraf/bin dist && node ./node_modules/webpack/bin/webpack --mode=production",
    "codecov": "./node_modules/.bin/codecov"
  }
}
